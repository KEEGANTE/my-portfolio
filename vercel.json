{
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/(.*)",
        "destination": "/index.html",
        "statusCode": 200
      }
    ]
  }
  